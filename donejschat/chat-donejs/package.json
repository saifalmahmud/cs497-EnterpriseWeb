{
  "name": "broncochat",
  "version": "0.0.0",
  "description": "An awesome chat application in donejs",
  "homepage": "broncochat",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saifalmahmud/broncochat.git"
  },
  "author": {
    "name": "Saif Al Mahmud",
    "email": "saifalmahmud@u.boisestate.edu",
    "url": ""
  },
  "private": true,
  "scripts": {
    "test": "testee test.html --browsers firefox --reporter Spec",
    "start": "done-serve --port 8080",
    "develop": "done-serve --develop --port 8080",
    "build": "node build",
    "deploy": "firebase deploy",
    "deploy:ci": "firebase deploy --token \"$FIREBASE_TOKEN\""
  },
  "main": "broncochat/index.stache!done-autorender",
  "files": [
    "src"
  ],
  "keywords": [
    "",
    "donejs-app"
  ],
  "steal": {
    "main": "broncochat/index.stache!done-autorender",
    "directories": {
      "lib": "src"
    },
    "configDependencies": [
      "live-reload",
      "node_modules/can-zone/register"
    ],
    "plugins": [
      "done-css",
      "done-component",
      "steal-less",
      "steal-stache"
    ],
    "envs": {
      "server-production": {
        "renderingBaseURL": "https://broncochat-saifalmahmud.firebaseapp.com/"
      }
    },
    "serviceBaseURL": "https://chat.donejs.com"
  },
  "dependencies": {
    "bit-tabs": "^1.0.4",
    "bootstrap": "^3.3.7",
    "can-component": "^3.3.5",
    "can-connect": "^1.5.9",
    "can-define": "^1.5.3",
    "can-route": "^3.2.3",
    "can-route-pushstate": "^3.1.2",
    "can-set": "^1.3.2",
    "can-stache": "^3.11.1",
    "can-view-autorender": "^3.1.1",
    "can-zone": "^0.6.13",
    "done-autorender": "^1.4.0",
    "done-component": "^1.0.0",
    "done-css": "^3.0.1",
    "done-serve": "^1.5.0",
    "generator-donejs": "^1.0.7",
    "steal": "^1.5.15",
    "steal-less": "^1.2.0",
    "steal-socket.io": "^4.0.9",
    "steal-stache": "^3.1.2"
  },
  "devDependencies": {
    "can-fixture": "^1.1.1",
    "donejs-cli": "^1.0.0",
    "firebase-tools": "^3.15.3",
    "funcunit": "^3.2.0",
    "steal-qunit": "^1.0.1",
    "steal-tools": "^1.9.1",
    "testee": "^0.3.0"
  }
}
